<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefHeight="520.0" prefWidth="886.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.frogdevelopment.assoplus.controller.ImportMembersController">
    <center>
        <TableView fx:id="table" managed="true" prefHeight="400.0" prefWidth="1045.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="actionCol" sortable="false" text="%member.actions">
                </TableColumn>
                <TableColumn text="%member.student.number">
                    <cellValueFactory>
                        <PropertyValueFactory property="studentNumber" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.lastname">
                    <cellValueFactory>
                        <PropertyValueFactory property="lastname" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.firstname">
                    <cellValueFactory>
                        <PropertyValueFactory property="firstname" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.birthday">
                    <cellValueFactory>
                        <PropertyValueFactory property="birthday" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.email">
                    <cellValueFactory>
                        <PropertyValueFactory property="email" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.degree">
                    <cellValueFactory>
                        <PropertyValueFactory property="degreeCode" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.option">
                    <cellValueFactory>
                        <PropertyValueFactory property="optionCode" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.phone">
                    <cellValueFactory>
                        <PropertyValueFactory property="phone" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </center>
    <top>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="162.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="518.0" minWidth="10.0" prefWidth="251.0" />
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="176.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="667.0" minWidth="10.0" prefWidth="274.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>

                <Button mnemonicParsing="false" onAction="#importMembers" text="Button" GridPane.columnIndex="2" GridPane.rowIndex="4" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.rowIndex="1" />
                <Label text="%member.lastname" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField GridPane.columnIndex="2" GridPane.rowIndex="1" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                <Label text="%member.firstname" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                <TextField GridPane.columnIndex="5" GridPane.rowIndex="1" />

                <CheckBox disable="true" mnemonicParsing="false" selected="true" />
                <Label text="%member.student.number" GridPane.columnIndex="1" />
                <TextField GridPane.columnIndex="2" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.columnIndex="3" />
                <Label text="%member.birthday" GridPane.columnIndex="4" />
                <TextField GridPane.columnIndex="5" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.rowIndex="2" />
                <Label text="%member.email" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField GridPane.columnIndex="2" GridPane.rowIndex="2" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                <Label text="%member.phone" GridPane.columnIndex="4" GridPane.rowIndex="2" />
                <TextField GridPane.columnIndex="5" GridPane.rowIndex="2" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.rowIndex="3" />
                <Label text="%member.degree" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField GridPane.columnIndex="2" GridPane.rowIndex="3" />

                <CheckBox mnemonicParsing="false" selected="true" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                <Label text="%member.option" GridPane.columnIndex="4" GridPane.rowIndex="3" />
                <TextField GridPane.columnIndex="5" GridPane.rowIndex="3" />
            </children>


        </GridPane>
    </top>

</BorderPane>
