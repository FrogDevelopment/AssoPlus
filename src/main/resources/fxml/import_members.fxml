<?import javafx.scene.control.Button?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<BorderPane prefHeight="520.0" prefWidth="886.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="child"
            fx:controller="fr.frogdevelopment.assoplus.controller.ImportMembersController">
    <top>
        <GridPane fx:id="top" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="162.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="518.0" minWidth="10.0" prefWidth="251.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="176.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="667.0" minWidth="10.0" prefWidth="274.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>

            <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" GridPane.columnSpan="6"
                     GridPane.rowIndex="4">
                <Button mnemonicParsing="false" onMouseClicked="#importMembers" text="%import.file"/>
            </ToolBar>

            <CheckBox fx:id="cbLastname" mnemonicParsing="false" selected="true" GridPane.rowIndex="1"/>
            <Label text="%member.lastname" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <TextField fx:id="tfLastname" GridPane.columnIndex="2" GridPane.rowIndex="1"/>

            <CheckBox fx:id="cbFirstname" mnemonicParsing="false" selected="true" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
            <Label text="%member.firstname" GridPane.columnIndex="4" GridPane.rowIndex="1"/>
            <TextField fx:id="tfFirstname" GridPane.columnIndex="5" GridPane.rowIndex="1"/>

            <CheckBox fx:id="cbStudentNumber" disable="true" mnemonicParsing="false" selected="true"/>
            <Label text="%member.student.number" GridPane.columnIndex="1"/>
            <TextField fx:id="tfStudentNumber" GridPane.columnIndex="2"/>

            <CheckBox fx:id="cbBirthday" mnemonicParsing="false" selected="true" GridPane.columnIndex="3"/>
            <Label text="%member.birthday" GridPane.columnIndex="4"/>
            <TextField fx:id="tfBirthday" GridPane.columnIndex="5"/>

            <CheckBox fx:id="cbEmail" mnemonicParsing="false" selected="true" GridPane.rowIndex="2"/>
            <Label text="%member.email" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <TextField fx:id="tfEmail" GridPane.columnIndex="2" GridPane.rowIndex="2"/>

            <CheckBox fx:id="cbPhone" mnemonicParsing="false" selected="true" GridPane.columnIndex="3" GridPane.rowIndex="2"/>
            <Label text="%member.phone" GridPane.columnIndex="4" GridPane.rowIndex="2"/>
            <TextField fx:id="tfPhone" GridPane.columnIndex="5" GridPane.rowIndex="2"/>

            <CheckBox fx:id="cbDegree" mnemonicParsing="false" selected="true" GridPane.rowIndex="3"/>
            <Label text="%member.degree" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            <TextField fx:id="tfDegree" GridPane.columnIndex="2" GridPane.rowIndex="3"/>

            <CheckBox fx:id="cbOption" mnemonicParsing="false" selected="true" GridPane.columnIndex="3" GridPane.rowIndex="3"/>
            <Label text="%member.option" GridPane.columnIndex="4" GridPane.rowIndex="3"/>
            <TextField fx:id="tfOption" GridPane.columnIndex="5" GridPane.rowIndex="3"/>
        </GridPane>
    </top>

    <center>
        <TableView fx:id="tableView" managed="true" prefHeight="400.0" prefWidth="1045.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="selectCol" maxWidth="20" minWidth="20" prefWidth="20" resizable="false" sortable="false">
                </TableColumn>
                <TableColumn text="%member.student.number">
                    <cellValueFactory>
                        <PropertyValueFactory property="studentNumber" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.lastname">
                    <cellValueFactory>
                        <PropertyValueFactory property="lastname" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.firstname">
                    <cellValueFactory>
                        <PropertyValueFactory property="firstname" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.birthday">
                    <cellValueFactory>
                        <PropertyValueFactory property="birthday" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.email">
                    <cellValueFactory>
                        <PropertyValueFactory property="email" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.degree">
                    <cellValueFactory>
                        <PropertyValueFactory property="degreeCode" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.option">
                    <cellValueFactory>
                        <PropertyValueFactory property="optionCode" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%member.phone">
                    <cellValueFactory>
                        <PropertyValueFactory property="phone" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </center>

    <bottom>
        <ToolBar fx:id="toolBar" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0"
                 BorderPane.alignment="CENTER">
            <Button mnemonicParsing="false" text="%import.import"/>
        </ToolBar>
    </bottom>

</BorderPane>
